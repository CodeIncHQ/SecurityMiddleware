{
  "name": "codeinc/security-middleware",
  "version": "1.0.0",
  "description": "A collection of PSR-15 middleware to manage HTTP security",
  "homepage": "https://github.com/CodeIncHQ/SecurityMiddleware",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "codeinc/psr7-responses": "^1.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "guzzlehttp/psr7": "^1.4"
  },
  "autoload": {
    "psr-4": {"CodeInc\\SecurityMiddleware\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\SecurityMiddleware\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  },
  "suggest": {
    "codeinc/middleware-dispatcher": "A PSR-15 middleware dispatcher",
    "codeinc/router": "A PSR-7 & PSR-15 router",
    "codeinc/psr7-response-sender": "A PSR-7 response sender"
  }
}